              <span class="spacer-big"></span>
        		<span id="high2"><img src="{{ 'high2.gif' | asset_url }}"></span>
              <span class="spacer-big"></span>
        		<span id="high3"><img src="{{ 'high3.gif' | asset_url }}"></span>

        	</div>
        </div>
  <span class="spacer-big"></span>
